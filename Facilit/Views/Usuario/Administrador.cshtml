@model IEnumerable<Facilit.Models.Usuario>

@{
    ViewBag.Title = "Administrador";
}

<h2>Administrador</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome_completo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nome_Usuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Senha_Usuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Excluido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Adm)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nome_completo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nome_Usuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Senha_Usuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Excluido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Adm)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
