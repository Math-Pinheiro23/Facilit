@model IEnumerable<Facilit.Models.Usuario>

@{
    ViewBag.Title = "Perfis_usuario";
}



    <h2>Todos os usuários</h2>


<input />
    
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome_completo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nome_Usuario)
            </th>
     
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nome_completo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Nome_Usuario)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Senha_Usuario)
                </td>

                <td>
                    @Html.ActionLink("Editar", "Editar", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) 
                </td>
            </tr>
        }
    </table>
<p>
   @Html.ActionLink("<---- Resgistar seu produto", "Registro", "Webcan")
</p>
