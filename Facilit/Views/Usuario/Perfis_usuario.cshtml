@model IEnumerable<Facilit.Models.Usuario>

@{
    ViewBag.Title = "Perfis_usuario";
}



    <h2>Todos os usuários</h2>


<input />
    
    <table class="table">
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nome completo</th>
            <th scope="col">E-mail</th>
            <th scope="col">Nome de usuário</th>
            <th scope="col">Data de cadastro</th>

            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Nome_completo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Nome_Usuario)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Criado)
                </td>

                <td>
                    @Html.ActionLink("Editar", "Editar", new { id = item.Id }) |
                    @Html.ActionLink("Visualizar", "Details", new { id = item.Id })


                </td>
            </tr>
        }
    </table>
<p>
   @Html.ActionLink("<---- Resgistar seu produto", "Registro", "Webcan")
</p>
