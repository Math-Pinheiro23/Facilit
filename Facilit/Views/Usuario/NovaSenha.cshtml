@model Facilit.Models.Usuario

@{
    ViewBag.Title = "Nova Senha";
}

<h2 style="text-align:center">Nova Senha</h2>

<hr />

<div class="form-group">
    @Html.LabelFor(model => model.Email, htmlAttributes: new { @class = "control-label col-md-6" })
